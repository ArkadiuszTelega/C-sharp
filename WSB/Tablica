using System;
using System.Collections.Generic;
using System.Linq;
using System.Text;
using System.Threading.Tasks;

namespace Tablice
{
	class Program
	{
		static void Main(string[] args)
		{
			// T a b l i c e //
			//Napisz program który w 10-elementowej tablicy jednowymiarowej o nazwie k umieszcza liczby całkowite z przedziału od 1 do 10
			int i = 10;
			//deklaracja tablicy o nazwie K typu całkowitego o rozmiarze n = 10
			int[] tab = new int[i];

			for (i = 0; i < tab.Length; i++)
			{
				tab[i] = i;
			}

			//wyświetlenie zawartości tablicy

			for (i = 0; i < tab.Length; i++)
			{
				Console.WriteLine("{0} ",tab[i]);
			}
			Console.ReadKey();

		}
	}
}
